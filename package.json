{
  "name": "code-comet-io",
  "version": "0.0.0",
  "scripts": {
    "start": "ng serve --open",
    "clean": "rm -rf dist/ node_modules/.cache",
    "build:meteor": "ng build meteor",
    "build:meteorite": "ng build meteorite --base-href /CodeCometIO/meteorite/",
    "build:code-comet-io": "ng build code-comet-io --base-href /CodeCometIO/",
    "prebuild:prod": "npm run clean && rm -rf dist/",
    "build:prod": "npm run build:meteor -- --prod && npm run build:meteorite -- --prod && npm run build:code-comet-io -- --prod",
    "postbuild:prod": "cp -r dist/code-comet-io dist/deploy && cp -r dist/meteorite  dist/deploy/meteorite/"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.0.3",
    "@angular/common": "~10.0.3",
    "@angular/compiler": "~10.0.3",
    "@angular/core": "~10.0.3",
    "@angular/forms": "~10.0.3",
    "@angular/platform-browser": "~10.0.3",
    "@angular/platform-browser-dynamic": "~10.0.3",
    "@angular/router": "~10.0.3",
    "prismjs": "^1.20.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.2",
    "@angular-devkit/build-ng-packagr": "~0.1000.2",
    "@angular/cli": "~10.0.2",
    "@angular/compiler-cli": "~10.0.3",
    "@angular/language-service": "~10.0.3",
    "@types/node": "^12.12.50",
    "@types/prismjs": "^1.16.1",
    "codelyzer": "^6.0.0",
    "cz-conventional-changelog": "^3.2.0",
    "ng-packagr": "^10.0.2",
    "ts-node": "~8.10.2",
    "tslint": "~6.1.2",
    "typescript": "~3.9.6"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
